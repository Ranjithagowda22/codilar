<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>

        <css src="Codilar_HelloWorld::css/css.css"/>

    </head>
    <update handle="customer_account"/>
<body>
    <referenceBlock name="page.main.title">
        <action method="setPageTitle">
            <argument translate="true" name="title" xsi:type="string">ADD PRODUCTS</argument>
        </action>
    </referenceBlock>
    <referenceContainer name="content">
    <block class="Codilar\HelloWorld\Block\HelloWorld"
           name="seller.adding.products"
           template="Codilar_HelloWorld::hello_world.phtml"
           cacheable="false"/>
</referenceContainer>
</body>
</page>